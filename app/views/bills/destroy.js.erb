// it will differentiate between admin and user bills for ajax calling
<% if current_user.role_id==1  %>
	// It will display bill for user
	$("#admin_page").html("<%= escape_javascript(render(:partial => "index")) %>");
<% else %>
	// It will display bill for admin
	$("#user_page").html("<%= escape_javascript(render(:partial => "index")) %>");
<% end %>